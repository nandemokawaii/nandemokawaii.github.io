-wrap pb-48\">\r\n        <div\r\n          v-for=\"items in items\"\r\n          :key=\"items.id\"\r\n          class=\"w-full xl:w-3/12 mb-12 xl:mb-0 px-1\"\r\n        >\r\n          <card-product\r\n            :title=\"items.title\"\r\n            :desc=\"items.desc\"\r\n            :src=\"items.img\"\r\n            :id=\"items.id\"\r\n            :category=\"items.category\"\r\n          />\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <div v-else>\r\n      <Spinner />\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport CardProduct from \"@/components/Cards/CardProduct.vue\";\r\nimport Spinner from \"@/components/Spinners/Spinner.vue\";\r\nimport RoundButton from \"@/components/Buttons/RoundButton.vue\";\r\nimport getItems from \"@/composables/getItems\";\r\n\r\nexport default {\r\n  name: \"shop-page\",\r\n  data() {\r\n    return {\r\n      url: '',\r\n      urlItems: \"http://localhost:3000/items\",\r\n      urlItemsByCategory: \"http://localhost:3000/items?category=\",\r\n    };\r\n  },\r\n  components: {\r\n    CardProduct,\r\n    Spinner,\r\n    RoundButton,\r\n  },\r\n  setup(){\r\n    const { items, error } = getItems()    \r\n    return { items, error }\r\n  },\r\n  methods: {\r\n    listByCategory(a) {\r\n      if(this.url != null){\r\n        this.url\r\n        console.log('yes')\r\n      }\r\n      else{\r\n        this.url = a != undefined ? this.urlItemsByCategory+a : this.urlItems\r\n        console.log('no')\r\n      }\r\n      \r\n      fetch(this.url)\r\n      .then((res) => res.json())\r\n      .then((data) => (this.items = data))\r\n      .catch((err) => console.log(err.message));\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style scoped>\r\n</style>\r\n"]}]}